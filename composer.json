{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/doctrine-orm": "^4.1",
    "api-platform/symfony": "^4.1",
    "avadim/fast-excel-writer": "^6.6",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.14",
    "doctrine/doctrine-migrations-bundle": "^3.4",
    "doctrine/orm": "^3.3",
    "gesdinet/jwt-refresh-token-bundle": "^1.4",
    "lexik/jwt-authentication-bundle": "^3.1",
    "nelmio/cors-bundle": "^2.5",
    "phpdocumentor/reflection-docblock": "^5.6",
    "phpstan/phpdoc-parser": "^2.1",
    "pontedilana/weasyprint-bundle": "*",
    "scheb/2fa-bundle": "^7.7",
    "scheb/2fa-email": "^7.7",
    "stripe/stripe-php": "^17.3",
    "symfony/asset": "7.4.*",
    "symfony/console": "7.4.*",
    "symfony/dotenv": "7.4.*",
    "symfony/expression-language": "7.4.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "7.4.*",
    "symfony/http-client": "7.4.*",
    "symfony/mailer": "7.4.*",
    "symfony/mercure-bundle": "^0.3.9",
    "symfony/property-access": "7.4.*",
    "symfony/property-info": "7.4.*",
    "symfony/rate-limiter": "7.4.*",
    "symfony/resend-mailer": "7.4.*",
    "symfony/runtime": "7.4.*",
    "symfony/security-bundle": "7.4.*",
    "symfony/serializer": "7.4.*",
    "symfony/twig-bundle": "7.4.*",
    "symfony/validator": "7.4.*",
    "symfony/yaml": "7.4.*",
    "tilleuls/forgot-password-bundle": "^2.0",
    "twig/extra-bundle": "^3.21",
    "vich/uploader-bundle": "^2.7"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "app:test": [
      "bin/console d:d:c --env=test",
      "bin/console d:m:m --no-interaction --allow-no-migration --env=test",
      "bin/console cache:clear --env=test",
      "bin/phpunit",
      "bin/console d:d:d --force --env=test"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.4.*",
      "docker": false
    }
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^8.3",
    "doctrine/doctrine-fixtures-bundle": "^4.1",
    "fakerphp/faker": "^1.24",
    "friendsofphp/php-cs-fixer": "^3.88",
    "justinrainbow/json-schema": "^6.5",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.4",
    "symfony/browser-kit": "7.4.*",
    "symfony/css-selector": "7.4.*",
    "symfony/maker-bundle": "^1.62",
    "symfony/phpunit-bridge": "^8.0",
    "symfony/stopwatch": "7.4.*",
    "symfony/web-profiler-bundle": "7.4.*",
    "zenstruck/foundry": "^2.7"
  }
}
